\profile "Rust Profile";

\problem {
\<{
  let n: u32 = 1u32;
  if let 1u32..10u32 = n {
    n
  };
  1u32
}\> true
}

\proof {
(branch "dummy ID"
(rule "letIdentPatAssign" (formula "1"))
(rule "letIdentPat" (formula "1"))
(rule "assignment" (formula "1"))
(rule "ifLetExclusive" (formula "1") (term "1"))
 (opengoal "==>  {n:=Z(1(#))}    \\<{   let b: bool = 1u32 <= n && n < 10u32;   if b {        n      };      1u32    }\\> true")
)
}
