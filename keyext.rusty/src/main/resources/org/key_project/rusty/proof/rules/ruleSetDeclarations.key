    defOps_expandModulo;
    defOps_expandJNumericOp;
    defOps_modHomoEq;

    // secondary categories for defined operations
    defOps_divModPullOut;

    // for equality versions of rules
    no_self_application;
    find_term_not_in_assumes;

    boxDiamondConv;

    // primary categories
    moveQuantToLeft;
    negationNormalForm;
    conjNormalForm;
    apply_equations_andOr;
    elimQuantifier;
