\sorts {
    \generic E0, E1, E2, E3, E4, E5, E6, E7, E8, E9, E10, E11;
}

\rules {
    set_tpl2_0 {
        \schemaVar \term Tuple2<[E0, E1]> t;
        \schemaVar \term E0 e;

        \find(set_tpl2_0<[E0, E1]>(t, e))
        \replacewith(tpl2<[E0, E1]>(e, tpl2_1<[E0, E1]>(t)))
    };

    set_tpl2_1 {
        \schemaVar \term Tuple2<[E0, E1]> t;
        \schemaVar \term E1 e;

        \find(set_tpl2_1<[E0, E1]>(t, e))
        \replacewith(tpl2<[E0, E1]>(tpl2_0<[E0, E1]>(t), e))
    };
}
