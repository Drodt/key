\sorts {
    \generic E0, E1, E2, E3, E4, E5, E6, E7, E8, E9, E10, E11;
}

\rules {
    get_of_tpl2_1 {
        \schemaVar \term Tuple2<[E0, E1]> t;
        \schemaVar \term E0 e0;
        \schemaVar \term E1 e1;

        \find(get<[Tuple2<[E0, E1]>, E1]>(tpl2<[E0, E1]>(e0, e1), Tuple$1<[E1]>))
        \replacewith(e1)
    };

    set_of_tpl2_0 {
        \schemaVar \term Tuple2<[E0, E1]> t;
        \schemaVar \term E0 e, e0;
        \schemaVar \term E1 e1;

        \find(set<[Tuple2<[E0, E1]>, E0]>(tpl2<[E0, E1]>(e0, e1), Tuple$0<[E0]>, e))
        \replacewith(tpl2<[E0, E1]>(e, e1))
    };
}
