\functions {
  int n;
  int m;
}
\problem {
   geq(m, Z(0(#))) & geq(n, m)
->   bsum{int i;}(Z(0(#)), n, i)
   = add(bsum{int i;}(Z(0(#)), m, i),
         bsum{int i;}(m, n, i))
}

